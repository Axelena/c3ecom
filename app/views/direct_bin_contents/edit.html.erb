<% provide(:title, "Edit bin contents") %>
<h2>Edit Item:</h2>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@direct_bin_contents) do |f| %>
        <%= render 'shared/error_messages_dbc' %>

        <%= f.label :item_code %>
        <%= f.text_field :item_code, :disabled=>true %>


        <%= f.label :bin_name %>
        <%= f.text_field :bin_name, :autofocus => true %>

        <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>

  </div>
</div>