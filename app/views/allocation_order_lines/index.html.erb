<% provide(:title, 'Allocation Order Lines') %>
<h1>Allocation Order Lines</h1>




<ul class="users">
  <table class="table table-striped">
    <tr>
      <th> Order # </th>
      <th> Item Code </th>
      <th> Qty Required </th>
      <th> Qty Scanned </th>
    </tr>

    <% @allocation_order_lines.each do |allocation_order_lines| %>
        <tr>
          <td><%= allocation_orders_lines.order_num %></td>
          <td><%= allocation_orders_lines.item_code %></td>
          <td><%= allocation_orders_lines.qty_required %></td>
          <td><%= allocation_orders_lines.qty_scanned %></td>
        </tr>
    <% end %>
  </table>
</ul>

